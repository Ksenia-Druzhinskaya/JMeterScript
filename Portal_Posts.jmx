            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Количество пользователей: берем из property userCount, конвертируем в int и увеличиваем на 1
int userQuantity = Integer.parseInt(props.get(&quot;userCount&quot;)) + 1;
log.info(&quot;User quantity: &quot; + userQuantity);

// Номер потока получаем из контекста JMeter (начинается с 0)
int threadNumber = ctx.getThreadNum();
log.info(&quot;User thread number: &quot; + threadNumber);

// Счетчик токенов: если текущий номер потока &lt; количества пользователей, то префикс токена = номеру потока
// иначе делим номера потока на количество пользователей, берем остаток
int index = ( threadNumber &lt; userQuantity ) ? threadNumber : threadNumber % userQuantity;
log.info(&quot;User token index: &quot; + index);

// Получаем токен из property
String token = props.get(&quot;userToken&quot; + index);

// Записываем токен в переменную JMeter, чтобы можно было использовать ${token} в запросах
vars.put(&quot;token&quot;, token);
log.info(&quot;Token: &quot; + token);</stringProp>
